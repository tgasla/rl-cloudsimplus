{
  "code-block-style": {
    "style": "fenced"
  },
  "code-fence-style": {
    "style": "backtick"
  },
  "emphasis-style": {
    "style": "asterisk"
  },
  "fenced-code-language": {
    "allowed_languages": [ "bash", "HTML", "markdown", "text" ],
    "language_only": true
  }
  "no-inline-html": {
    "allowed_elements": [ "a", "b", "div", "details", "p" ]
  }
}
